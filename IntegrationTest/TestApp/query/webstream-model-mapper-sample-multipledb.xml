<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC
  "-//github.com/mapserver2007//DTD Mapper 3.0//EN"
  "http://localhost/WebStream/core/WebStream/Test/Sample/custom/webstream-model-mapper.dtd">
<mapper namespace="WebStream\Test\TestData\Sample\App\Model">
  <select id="useMysql">
    SELECT
        *
    FROM
        T_WebStream
    LIMIT
        :limit, :offset
  </select>

  <select id="usePostgres">
    SELECT
        *
    FROM
        T_WebStream
    LIMIT
        :limit
    OFFSET
        :offset
  </select>

  <select id="useSqlite">
    SELECT
        *
    FROM
        T_WebStream
    LIMIT
        :limit
    OFFSET
        :offset
  </select>
</mapper>